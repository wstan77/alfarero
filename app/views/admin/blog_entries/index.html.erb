<p id="notice"><%= notice %></p>

<h1>Listing Admin Blog Entries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_blog_entries.each do |admin_blog_entry| %>
      <tr>
        <td><%= link_to 'Show', admin_blog_entry %></td>
        <td><%= link_to 'Edit', edit_admin_blog_entry_path(admin_blog_entry) %></td>
        <td><%= link_to 'Destroy', admin_blog_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Blog entry', new_admin_blog_entry_path %>
