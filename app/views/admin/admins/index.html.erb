<p id="notice"><%= notice %></p>

<h1>Listing Admin Admins</h1>

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_admins.each do |admin| %>
      <tr>
        <td><%= admin.full_name %></td>
        <td><%= admin.email %></td>
        <td><%= link_to 'Show', admin_admin_path(admin) %></td>
        <td><%= link_to 'Edit', edit_admin_admin_path(admin) %></td>
        <td><%= link_to 'Destroy', admin_admin_path(admin), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin', new_admin_admin_path %>
