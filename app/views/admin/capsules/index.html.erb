<p id="notice"><%= notice %></p>

<h1>Listing Admin Capsules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_capsules.each do |admin_capsule| %>
      <tr>
        <td><%= link_to 'Show', admin_capsule %></td>
        <td><%= link_to 'Edit', edit_admin_capsule_path(admin_capsule) %></td>
        <td><%= link_to 'Destroy', admin_capsule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Capsule', new_admin_capsule_path %>
